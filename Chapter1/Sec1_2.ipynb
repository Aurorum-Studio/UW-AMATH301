   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "dx = 0.1\n",
    "x = np.arange(-10,10+dx,dx)\n",
    "y = np.exp(x)-np.tan(x)\n",
    "y2 = 0*y # plot the y=0 line\n",
    "\n",
    "plt.plot(x,y)\n",
    "plt.plot(x,y2)\n",
    "plt.axis([-10, 10, -10, 20])\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Algorithm for bisection"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "-3.0964111328124995"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    },
    {
     "data": {
      "text/plain": [
       "13"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "xr = -2.8; xl = -4\n",
    "for j in range(0, 100):\n",
    "    xc = (xr + xl)/2\n",
    "    fc = np.exp(xc) - np.tan(xc)   \n",
    "    if ( fc > 0 ):\n",
    "        xl = xc   \n",
    "    else:\n",
    "        xr = xc\n",
    "        \n",
    "    if ( abs(fc) < 1e-5 ):        \n",
    "        display(xc); display(j)\n",
    "        break"
   ]
